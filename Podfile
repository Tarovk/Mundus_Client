# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'Mundus_ios' do
    # Comment the next line if you're not using Swift and don't want to use dynamic
    use_frameworks!

    pod 'Aldo', :git => 'https://github.com/makchich/aldo-api-client-library-ios.git', :branch => 'develop'
    pod 'Starscream', '~> 2.0.0'
    pod 'SwiftSpinner'
    pod 'SKPhotoBrowser'
    pod 'Toaster'
    
    # Pods for Mundus_ios
    target 'Mundus_iosTests' do
        inherit! :search_paths
        # Pods for testing
    end

    target 'Mundus_iosUITests' do
        inherit! :search_paths
        # Pods for testing
    end
end
