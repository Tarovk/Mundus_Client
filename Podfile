  pod "Toaster"
  # Pods for Mundus_ios
  target 'Mundus_iosTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Mundus_iosUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
